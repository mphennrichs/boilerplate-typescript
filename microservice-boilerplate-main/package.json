  "main": "dist/src/boilerplate/shared/index.js",
  "types": "dist/src/boilerplate/shared/index.d.ts",
  "files": [
    "dist/src/boilerplate/shared/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pagaleve/microservice-boilerplate.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "workspaces": [
    "shared",
    "application"
  ],
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "scripts": {
    "gen-doc": "pnpm -r run gen-doc",
    "prestart": "pnpm -r run prestart",
    "prebuild": "pnpm -r run prebuild",
    "postinstall": "pnpm run --filter @pagaleve/boilerplate-service-shared build",
    "build": "pnpm run --filter @pagaleve/boilerplate-service-shared build && pnpm run --filter @pagaleve/microservice-boilerplate build",
    "off": "pnpm -r run off",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm -r run test",
    "test:debug": "pnpm -r run test:debug",
    "test:unit": "pnpm -r run test:unit",
    "test:unit:watch": "pnpm -r run test:unit:watch",
    "test:e2e": "pnpm -r run test:e2e",
    "test:e2e:watch": "pnpm -r run test:e2e:watch",
    "test:staged": "pnpm run --filter @pagaleve/microservice-boilerplate test:staged",
    "test:ci": "pnpm -r run test:ci",
    "deploy": "pnpm run --filter @pagaleve/microservice-boilerplate deploy",
    "migration": "cd ./application && pnpm run migration",
    "migration:local": "cd ./application &&  migration:local",
    "gen-doc-local": "cd ./application && gen-doc-local",
    "update-deps": "npm-check -s -u"
  },
  "dependencies": {
