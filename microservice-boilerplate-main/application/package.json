  "scripts": {
    "build": "(rimraf dist || true) && ts-patch install && nest build -p tsconfig.json",
    "buildPrePublish": "(rimraf dist || true) && ts-patch install && tsc -p tsconfig.build.json",
    "debug": "rimraf .webpack && sls offline --stage",
    "deploy": "SLS_DEBUG=\"*\" sls deploy --stage",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gen-doc": "AWS_XRAY_CONTEXT_MISSING=LOG_ERROR AWS_XRAY_LOG_LEVEL=silent node dist/src/doc",
    "gen-doc-local": "AWS_XRAY_CONTEXT_MISSING=LOG_ERROR AWS_XRAY_LOG_LEVEL=silent ts-node -r dotenv/config -r tsconfig-paths/register ./src/doc",
    "invoke:create": "sls invoke local -f main -p test/events-handler/merchant/create.json",
    "invoke:delete": "sls invoke local -f main -p test/events-handler/merchant/delete.json",
    "invoke:get": "sls invoke local -f main -p test/events-handler/merchant/get.json",
    "invoke:list": "sls invoke local -f main -p test/events-handler/merchant/list.json",
    "invoke:update": "sls invoke local -f main -p test/events-handler/merchant/update.json",
    "lint": "echo \"AQUI\" && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration": "SLS_DEBUG=\"*\" sls invoke -f migration --stage",
    "migration:local": "AWS_XRAY_CONTEXT_MISSING=LOG_ERROR AWS_XRAY_LOG_LEVEL=silent SLS_DEBUG=\"*\" sls invoke local -f migration -s",
    "off": "AWS_XRAY_CONTEXT_MISSING=LOG_ERROR AWS_XRAY_LOG_LEVEL=silent sls offline --stage ",
    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run buildPrePublish",
    "prestart": "rimraf dist",
    "redrive:local": "AWS_XRAY_CONTEXT_MISSING=LOG_ERROR AWS_XRAY_LOG_LEVEL=silent SLS_DEBUG=\"*\" sls invoke local -f dynamodbToElasticSearchRedrive -s",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "AWS_XRAY_CONTEXT_MISSING=LOG_ERROR AWS_XRAY_LOG_LEVEL=silent jest --passWithNoTests --runInBand --no-cache",
    "test:ci": "npm test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run test -- -c jest-e2e.config.js",
    "test:e2e:watch": "npm run test:e2e -- --watch",
    "test:staged": "npm run test:unit -- --findRelatedTests",
    "test:unit": "npm run test -- -c jest-unit.config.js",
    "test:unit:watch": "npm run test:unit -- --watch",
    "update-deps": "npm-check -s -u"
  },
  "version": "1.0.0"
